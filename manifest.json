{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Martin's Sample App",
    "version": "1.0.0",
    "minimum-overwolf-version":"0.77.10",
    "author": "Martin Ashby",
    "icon": "Icons/icon.png",
    "icon_gray": "Icons/icon.png",
    "description": "Demo App"
  },
  "data": {
    "start_window": "MainWindow",
    "windows": {
      "MainWindow": {
        "file": "Files/index.html",
        "transparent": true,
        "resizable": true,
        "size": {"width": 400, "height": 300},
        "min_size": {"width": 200, "height": 200},
        "max_size": {"width": 600, "height": 500},
        "externally_connectable":  {
          "matches": [
            "https://api.guildwars2.com"
          ]
        }
      }
    }
  }
}